<UserControl
    x:Class="Wbooru.UI.Controls.SettingUI.ListSetting"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Wbooru.UI.Controls.SettingUI"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006">

    <StackPanel Name="MainPanel" Orientation="Horizontal">
        <TextBlock
            Margin="0,0,15,0"
            VerticalAlignment="Center"
            Text="{Binding Wrapper.DisplayPropertyName, Mode=OneWay}" />
        <ComboBox
            Name="MainComboBox"
            Width="150"
            Template="{StaticResource NormalControlBoxTemplate}"
            Text="{Binding Text, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">

            <TextBlock>asdasdasd</TextBlock>
            <TextBlock>asdasdasd2</TextBlock>
        </ComboBox>
    </StackPanel>
</UserControl>
